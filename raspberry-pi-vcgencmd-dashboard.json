        {
          "expr": "label_replace({__name__=~\"vcgencmd_measure_clock_.*\",instance=\"$instance\"}, \"clock\", \"$1\", \"__name__\", \"vcgencmd_measure_clock_(.*)\")",
          "legendFormat": "{{clock}}",
          "refId": "A"
        }
      ],
      "title": "Clock Frequencies",
      "transformations": [],
      "transparent": false,
      "type": "timeseries"
    },
