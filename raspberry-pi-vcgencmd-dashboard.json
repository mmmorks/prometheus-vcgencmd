{
  "annotations": {},
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "description": "CPU and ring oscillator temperature readings",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 20,
            "lineInterpolation": "smooth",
            "lineWidth": 2
          },
          "max": 85,
          "min": 0,
          "unit": "celsius"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "legend": {
          "calcs": [
            "mean",
            "max"
          ],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "asc"
        }
      },
      "repeatDirection": "h",
      "targets": [
        {
          "expr": "label_replace(  label_replace(    {__name__=~\"vcgencmd_measure_temp|vcgencmd_read_ring_osc_temperature\",instance=\"$instance\"},     \"sensor\", \"CPU\", \"__name__\", \"vcgencmd_measure_temp\"),   \"sensor\", \"Ring Osc\", \"__name__\", \"vcgencmd_read_ring_osc_temperature\")",
          "legendFormat": "{{sensor}}",
          "refId": "A"
        }
      ],
      "title": "Temperature",
      "transformations": [],
      "transparent": false,
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "description": "Core, ring oscillator, and SDRAM voltages",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 20,
            "lineInterpolation": "smooth",
            "lineWidth": 2
          },
          "max": 1.4,
          "min": 0,
          "unit": "volt"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 2,
      "options": {
        "legend": {
          "calcs": [
            "mean",
            "max"
          ],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "asc"
        }
      },
      "repeatDirection": "h",
      "targets": [
        {
          "expr": "label_replace(  label_replace(    label_replace(      {__name__=~\"vcgencmd_measure_volts_.*|vcgencmd_read_ring_osc_volts\",instance=\"$instance\"},       \"voltage\", \"SDRAM $1\", \"__name__\", \"vcgencmd_measure_volts_sdram_([cip])\"),     \"voltage\", \"Core\", \"__name__\", \"vcgencmd_measure_volts_core\"),   \"voltage\", \"Ring Osc\", \"__name__\", \"vcgencmd_read_ring_osc_volts\")",
          "legendFormat": "{{voltage}}",
          "refId": "A"
        }
      ],
      "title": "Voltages",
      "transformations": [],
      "transparent": false,
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
